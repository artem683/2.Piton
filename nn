# 1 
n = 0
a = 18
b = 65
c= 19
h = 100
if a>n:
    print ('Вы не совешеннолетний')
if a<c<b: 
    print ('Вы трудоспособный человек')    
if b<h:
    print ('Вы пенсионер')
#2
a=1000
b=6000
c=5000
if b<a:
     print ('cкидка не предостовляется')
elif b>a:
     print ('скидка предоставляется')
if b>a<c: 
     print ('скидка 5%')
if b>c:     
     print ('cкидка 10%')
